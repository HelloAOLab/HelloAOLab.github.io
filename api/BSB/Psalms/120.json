{"translation":{"id":"BSB","name":"Berean Standard Bible","englishName":"Berean Standard Bible","shortName":"BSB","language":"en-US","licenseUrl":"https://berean.bible/terms.htm","website":"https://berean.bible","availableFormats":["json"],"listOfBooksApiLink":"/api/BSB/books.json"},"book":{"id":"PSA","name":"Psalm","commonName":"Psalms","title":"Psalm","firstChapterApiLink":"/api/BSB/Psalms/1.json","lastChapterApiLink":"/api/BSB/Psalms/150.json","numberOfChapters":150},"nextChapterApiLink":"/api/BSB/Psalms/121.json","previousChapterApiLink":"/api/BSB/Psalms/119.json","chapter":{"number":120,"content":[{"type":"heading","content":["In My Distress I Cried to the LORD"]},{"type":"line_break"},{"type":"hebrew_subtitle","content":[{"text":"A song of ascents.","poem":2}]},{"type":"line_break"},{"type":"verse","number":1,"content":[{"text":"In my distress I cried to the LORD,","poem":1},{"text":"and He answered me.","poem":2}]},{"type":"verse","number":2,"content":[{"text":"Deliver my soul, O LORD,","poem":1},{"text":"from lying lips and a deceitful tongue.","poem":2}]},{"type":"line_break"},{"type":"verse","number":3,"content":[{"text":"What will He do to you,","poem":1},{"text":"and what will be added to you,","poem":2},{"text":"O deceitful tongue?","poem":2}]},{"type":"verse","number":4,"content":[{"text":"Sharp arrows will come from the warrior,","poem":1},{"text":"with burning coals of the broom tree!","poem":2}]},{"type":"line_break"},{"type":"verse","number":5,"content":[{"text":"Woe to me that I dwell in Meshech,","poem":1},{"text":"that I live among the tents of Kedar!","poem":2}]},{"type":"verse","number":6,"content":[{"text":"Too long have I dwelt","poem":1},{"text":"among those who hate peace.","poem":2}]},{"type":"verse","number":7,"content":[{"text":"I am in favor of peace;","poem":1},{"text":"but when I speak, they want war.","poem":2}]}],"footnotes":[]}}