{"translation":{"id":"BSB","name":"Berean Standard Bible","englishName":"Berean Standard Bible","shortName":"BSB","language":"en-US","licenseUrl":"https://berean.bible/terms.htm","website":"https://berean.bible","availableFormats":["json"],"listOfBooksApiLink":"/api/BSB/books.json","textDirection":"ltr"},"book":{"id":"PSA","name":"Psalm","commonName":"Psalms","title":"Psalm","firstChapterApiLink":"/api/BSB/Psalms/1.json","lastChapterApiLink":"/api/BSB/Psalms/150.json","numberOfChapters":150},"nextChapterApiLink":"/api/BSB/Psalms/101.json","previousChapterApiLink":"/api/BSB/Psalms/99.json","chapter":{"number":100,"content":[{"type":"heading","content":["Make a Joyful Noise"]},{"type":"line_break"},{"type":"hebrew_subtitle","content":[{"text":"A Psalm of thanksgiving.","poem":2}]},{"type":"line_break"},{"type":"verse","number":1,"content":[{"text":"Make a joyful noise to the LORD,","poem":1},{"text":"all the earth.","poem":2}]},{"type":"line_break"},{"type":"verse","number":2,"content":[{"text":"Serve the LORD with gladness;","poem":1},{"text":"come into His presence with joyful songs.","poem":2}]},{"type":"verse","number":3,"content":[{"text":"Know that the LORD is God.","poem":1},{"text":"It is He who made us, and we are His;","poem":2},{"noteId":211},{"text":"we are His people, and the sheep of His pasture.","poem":2}]},{"type":"verse","number":4,"content":[{"text":"Enter His gates with thanksgiving","poem":1},{"text":"and His courts with praise;","poem":2},{"text":"give thanks to Him and bless His name.","poem":2}]},{"type":"line_break"},{"type":"verse","number":5,"content":[{"text":"For the LORD is good,","poem":1},{"text":"and His loving devotion endures forever;","poem":2},{"text":"His faithfulness continues to all generations.","poem":2}]}],"footnotes":[{"noteId":211,"text":"Or and not we ourselves","reference":{"chapter":100,"verse":3}}]}}