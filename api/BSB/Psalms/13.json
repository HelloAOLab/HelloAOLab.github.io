{"translation":{"id":"BSB","name":"Berean Standard Bible","englishName":"Berean Standard Bible","shortName":"BSB","language":"en-US","licenseUrl":"https://berean.bible/terms.htm","website":"https://berean.bible","availableFormats":["json"],"listOfBooksApiLink":"/api/BSB/books.json"},"book":{"id":"PSA","name":"Psalm","commonName":"Psalms","title":"Psalm","firstChapterApiLink":"/api/BSB/Psalms/1.json","lastChapterApiLink":"/api/BSB/Psalms/150.json","numberOfChapters":150},"nextChapterApiLink":"/api/BSB/Psalms/14.json","previousChapterApiLink":"/api/BSB/Psalms/12.json","chapter":{"number":13,"content":[{"type":"heading","content":["How Long, O LORD?"]},{"type":"line_break"},{"type":"hebrew_subtitle","content":[{"text":"For the choirmaster. A Psalm of David.","poem":2}]},{"type":"line_break"},{"type":"verse","number":1,"content":[{"text":"How long, O LORD?","poem":1},{"text":"Will You forget me forever?","poem":2},{"text":"How long will You hide Your face from me?","poem":2}]},{"type":"verse","number":2,"content":[{"text":"How long must I wrestle in my soul,","poem":1},{"text":"with sorrow in my heart each day?","poem":2},{"text":"How long will my enemy dominate me?","poem":2}]},{"type":"line_break"},{"type":"verse","number":3,"content":[{"text":"Consider me and respond, O LORD my God.","poem":1},{"text":"Give light to my eyes, lest I sleep in death,","poem":2}]},{"type":"verse","number":4,"content":[{"text":"lest my enemy say, “I have overcome him,”","poem":1},{"text":"and my foes rejoice when I fall.","poem":2}]},{"type":"line_break"},{"type":"verse","number":5,"content":[{"text":"But I have trusted in Your loving devotion;","poem":1},{"text":"my heart will rejoice in Your salvation.","poem":2}]},{"type":"verse","number":6,"content":[{"text":"I will sing to the LORD,","poem":1},{"text":"for He has been good to me.","poem":2}]}],"footnotes":[]}}