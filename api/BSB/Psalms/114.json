{"translation":{"id":"BSB","name":"Berean Standard Bible","englishName":"Berean Standard Bible","shortName":"BSB","language":"en-US","licenseUrl":"https://berean.bible/terms.htm","website":"https://berean.bible","availableFormats":["json"],"listOfBooksApiLink":"/api/BSB/books.json"},"book":{"id":"PSA","name":"Psalm","commonName":"Psalms","title":"Psalm","firstChapterApiLink":"/api/BSB/Psalms/1.json","lastChapterApiLink":"/api/BSB/Psalms/150.json","numberOfChapters":150},"nextChapterApiLink":"/api/BSB/Psalms/115.json","previousChapterApiLink":"/api/BSB/Psalms/113.json","chapter":{"number":114,"content":[{"type":"heading","content":["A Psalm of Exodus"]},{"type":"line_break"},{"type":"verse","number":2,"content":[{"text":"Judah became Godâ€™s sanctuary,","poem":1},{"text":"and Israel His dominion.","poem":2}]},{"type":"line_break"},{"type":"verse","number":3,"content":[{"text":"The sea observed and fled;","poem":1},{"text":"the Jordan turned back;","poem":2}]},{"type":"verse","number":4,"content":[{"text":"the mountains skipped like rams,","poem":1},{"text":"the hills like lambs.","poem":2}]},{"type":"verse","number":5,"content":[{"text":"Why was it, O sea, that you fled,","poem":1},{"text":"O Jordan, that you turned back,","poem":2}]},{"type":"verse","number":6,"content":[{"text":"O mountains, that you skipped like rams,","poem":1},{"text":"O hills, like lambs?","poem":2}]},{"type":"line_break"},{"type":"verse","number":7,"content":[{"text":"Tremble, O earth, at the presence of the Lord,","poem":1},{"text":"at the presence of the God of Jacob,","poem":2}]},{"type":"verse","number":8,"content":[{"text":"who turned the rock into a pool,","poem":1},{"text":"the flint into a fountain of water!","poem":2}]}],"footnotes":[]}}